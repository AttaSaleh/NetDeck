{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NetDeck",
  "homepage_url": "http://netdeck.n4ru.it",
  "description": "Get hearthstone decklists from popular sites.",
  "version": "2.5",
  "icons": {
    "16": "key.png",
    "48": "key.png",
    "128": "key.png"
  },
  "browser_action": {
    "default_icon": "key.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "run_at": "document_end",
    "persistent": false
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "notifications",
    "storage"
  ],
  "commands": {
    "savedeck": {
      "suggested_key": {
        "windows": "Ctrl+Shift+S",
        "mac": "Command+Shift+S",
        "chromeos": "Ctrl+Shift+S",
        "linux": "Ctrl+Shift+S"
      },
    "description": "Save current deck"
    }
  },
  "web_accessible_resources": [
    "jq.js",
    "jDeck.js"
  ]
}